  "customFormulas": {
    "default-tenant": [
      {
        "id": "formula-1-default",
        "tenantId": "default-tenant",
        "name": "Performance Score",
        "variableId": "performance_score",
        "formula": "AVG(code_quality, team_collab) * 20",
        "description": "Average of code quality and team collaboration, scaled to 100",
        "displayFormat": "number",
        "category": "score",
        "createdAt": "2024-01-15T00:00:00.000Z",
        "updatedAt": "2024-07-10T00:00:00.000Z"
      },
      {
        "id": "formula-2-default",
        "tenantId": "default-tenant",
        "name": "Growth Potential",
        "variableId": "growth_potential",
        "formula": "IF(learning_agility > 8 AND performance_score > 80, \"High\", IF(learning_agility > 6 OR performance_score > 70, \"Medium\", \"Low\"))",
        "description": "Categorizes employee growth potential based on learning agility and performance",
        "displayFormat": "text",
        "category": "classification",
        "createdAt": "2024-02-01T00:00:00.000Z",
        "updatedAt": "2024-07-05T00:00:00.000Z"
      }
    ],
    "acme-corp": [
      {
        "id": "formula-1-acme",
        "tenantId": "acme-corp",
        "name": "Sales Effectiveness",
        "variableId": "sales_effectiveness",
        "formula": "IF(negotiation_skills > 7, \"High\", IF(negotiation_skills > 4, \"Medium\", \"Low\"))",
        "description": "Categorizes sales effectiveness based on negotiation skills",
        "displayFormat": "text",
        "category": "classification",
        "createdAt": "2024-02-20T00:00:00.000Z",
        "updatedAt": "2024-07-15T00:00:00.000Z"
      },
      {
        "id": "formula-2-acme",
        "tenantId": "acme-corp",
        "name": "Revenue Impact Score",
        "variableId": "revenue_impact",
        "formula": "(deals_closed * avg_deal_size) / quota * 100",
        "description": "Calculates revenue impact as percentage of quota achievement",
        "displayFormat": "percentage",
        "category": "performance",
        "createdAt": "2024-03-01T00:00:00.000Z",
        "updatedAt": "2024-07-12T00:00:00.000Z"
      }
    ],
    "tech-startup": [
      {
        "id": "formula-1-tech",
        "tenantId": "tech-startup",
        "name": "Startup Fit Score",
        "variableId": "startup_fit",
        "formula": "AVG(adaptability, ownership, innovation) * 10",
        "description": "Measures how well someone fits startup culture",
        "displayFormat": "number",
        "category": "culture",
        "createdAt": "2024-07-05T00:00:00.000Z",
        "updatedAt": "2024-07-18T00:00:00.000Z"
      }
    ],
    "localhost-tenant": [
      {
        "id": "formula-1-localhost",
        "tenantId": "localhost-tenant",
        "name": "Puntuación de Satisfacción",
        "variableId": "satisfaction_score",
        "formula": "AVG(calidad_servicio, tiempo_respuesta, facilidad_uso) * 20",
        "description": "Promedio de calidad de servicio, tiempo de respuesta y facilidad de uso, escalado a 100",
        "displayFormat": "number",
        "category": "score",
        "createdAt": "2024-07-10T00:00:00.000Z",
        "updatedAt": "2024-07-20T00:00:00.000Z"
      },
      {
        "id": "formula-2-localhost",
        "tenantId": "localhost-tenant",
        "name": "Nivel de Recomendación",
        "variableId": "recommendation_level",
        "formula": "IF(satisfaction_score > 85, \"Alto\", IF(satisfaction_score > 70, \"Medio\", \"Bajo\"))",
        "description": "Categoriza el nivel de recomendación basado en la puntuación de satisfacción",
        "displayFormat": "text",
        "category": "classification",
        "createdAt": "2024-07-12T00:00:00.000Z",
        "updatedAt": "2024-07-20T00:00:00.000Z"
      }
    ]
